{
    "compilerOptions": {
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "lib": [
            "dom",
            "dom.iterable",
            "es2015",
            "esnext.asynciterable",
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "jsx": "react",
        "strict": true,
        "downlevelIteration": true,
        "target": "ES2017",
        "types": ["node"]
    }
}
