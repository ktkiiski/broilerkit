    "@types/lodash": "^4.14.104",
    "@types/mime": "^1.3.1",
    "@types/nedb": "^1.8.5",
    "@types/node": "8.5",
    "@types/vinyl-fs": "^2.4.8",
    "@types/webpack": "^3.8.8",
    "@types/webpack-dev-server": "^2.9.3",
    "@types/yamljs": "^0.2.30",
    "awesome-typescript-loader": "^3.4.1",
    "aws-sdk": "^2.200.0",
    "chalk": "^2.3.1",
    "core-js": "^2.5.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "eslint": "^4.18.1",
