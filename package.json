    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "ulid": "^2.3.0",
    "url-loader": "^1.0.1",
    "vinyl-fs": "^3.0.2",
    "webapp-webpack-plugin": "^1.3.1",
