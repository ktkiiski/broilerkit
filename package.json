{
  "name": "broilerkit",
  "version": "0.4.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=8.2"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "find . -type d \\( -path ./node_modules -o -path ./types \\) -prune -o -type f \\( -name '*.js' -o -name '*.d.ts' -o -name '*.js.map' \\) -exec rm {} \\;",
    "clean-build": "npm run clean && npm run compile",
    "compile": "tsc -p .",
    "prepare": "npm run clean-build",
    "lint": "tslint --project .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktkiiski/broilerkit.git"
  },
  "keywords": [
    "broilerplate",
    "boilerplate",
    "aws"
  ],
  "author": {
    "name": "Kimmo Kiiski",
    "email": "kimmo@kii.ski",
    "url": "https://kii.ski"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktkiiski/broilerkit/issues"
  },
  "homepage": "https://github.com/ktkiiski/broilerkit#readme",
