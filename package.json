{
  "name": "broilerkit",
  "version": "0.7.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "index.js",
  "types": "index.ts",
  "engines": {
    "node": ">=8.2"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "find . -type d \\( -path ./node_modules -o -path ./types \\) -prune -o -type f \\( -name '*.js' -o -name '*.js.map' \\) -exec rm {} \\;",
    "clean-build": "npm run clean && npm run compile && npm run lint",
    "compile": "tsc -p .",
    "prepare": "npm run clean-build && npm run test",
    "lint": "tslint --project .",
    "test": "mocha -r ts-node/register spec/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktkiiski/broilerkit.git"
  },
  "keywords": [
    "broilerplate",
    "boilerplate",
    "aws"
  ],
  "author": {
    "name": "Kimmo Kiiski",
    "email": "kimmo@kii.ski",
    "url": "https://kii.ski"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktkiiski/broilerkit/issues"
  },
  "homepage": "https://github.com/ktkiiski/broilerkit#readme",
  "dependencies": {
    "@types/del": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/jszip": "3.1.4",
    "@types/mime": "^2.0.0",
    "@types/nedb": "^1.8.5",
    "@types/node": "9.6",
    "@types/vinyl-fs": "^2.4.8",
    "@types/webpack": "^3.8.8",
    "@types/webpack-dev-server": "2.9.4",
    "@types/yamljs": "^0.2.30",
    "aws-sdk": "^2.213.1",
    "chalk": "^2.3.2",
    "core-js": "^2.5.3",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^1.4.1",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "image-webpack-loader": "^4.2.0",
    "jsonwebtoken": "^8.4.0",
