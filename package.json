{
  "name": "broilerkit",
  "version": "0.0.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8.2"
  },
  "files": [
    "dist",
    "res",
    "src",
    "types"
  ],
  "bin": {
    "broil": "./dist/broil.js"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist",
    "clean-build": "npm run clean && npm run compile",
    "compile": "tsc -p . && chmod +x ./dist/broil.js",
    "prepare": "npm run clean-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
