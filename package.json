{
  "name": "broilerkit",
  "version": "0.25.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "index.js",
  "types": "index.ts",
  "bin": "./cmd.js",
  "engines": {
    "node": ">=12.14"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -f res/cloudformation-migration-lambda.zip && find . -type d \\( -path ./node_modules -o -path ./types \\) -prune -o -type f \\( -name '*.js' -o -name '*.js.map' -o -name '*.d.ts' \\) -exec rm {} \\;",
    "clean-build": "npm run clean && npm run lint && npm run compile",
    "compile": "tsc -p .",
    "zip-cf-migration-lambda": "zip res/cloudformation-migration-lambda.zip -r migration.js palette.js sql-log.js node_modules/pg node_modules/buffer-writer node_modules/packet-reader node_modules/pg-connection-string node_modules/pg-pool node_modules/pg-types node_modules/pg-int8 node_modules/postgres-array node_modules/postgres-bytea node_modules/postgres-date node_modules/postgres-interval node_modules/xtend node_modules/pgpass node_modules/split node_modules/through node_modules/semver",
    "prepare": "npm run clean-build && npm run mocha && npm run zip-cf-migration-lambda",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "mocha": "mocha -r ts-node/register spec/**/*.spec.ts",
    "pretest": "npm run clean",
    "test": "npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktkiiski/broilerkit.git"
  },
  "keywords": [
    "broilerplate",
    "boilerplate",
    "aws"
  ],
  "author": {
    "name": "Kimmo Kiiski",
    "email": "kimmo@kii.ski",
    "url": "https://kii.ski"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktkiiski/broilerkit/issues"
  },
  "homepage": "https://github.com/ktkiiski/broilerkit#readme",
  "sideEffects": [
    "./cmd.ts",
    "./cmd.js"
  ],
  "dependencies": {
    "@types/ansi-styles": "^3.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jszip": "3.4.1",
    "@types/mime": "^2.0.3",
    "@types/msgpack-lite": "^0.1.7",
    "@types/node": "^14.0.24",
    "@types/node-jose": "^1.1.4",
    "@types/pg": "^7.14.4",
    "@types/react": "^16.x",
    "@types/react-dom": "^16.x",
    "@types/react-router": "5.x",
    "@types/react-router-dom": "^5.x",
    "@types/supports-color": "^5.3.0",
    "@types/vinyl-fs": "^2.4.11",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "3.11.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "ansi-styles": "^4.2.1",
    "aws-sdk": "^2.718.0",
    "base64url": "^3.0.1",
    "core-js-pure": "^3.6.5",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.9",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "immuton": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.5.0",
    "mime": "^2.4.6",
    "mini-css-extract-plugin": "^0.9.0",
    "msgpack-lite": "^0.1.26",
    "node-jose": "^1.1.4",
    "pg": "^8.3.0",
    "pg-cursor": "^2.3.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "supports-color": "^7.1.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslib": "^1.11.1",
    "typescript": "~3.8.3",
    "ulid": "^2.3.0",
    "url-loader": "^4.1.0",
    "vinyl-fs": "^3.0.3",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.0",
    "yamljs": "^0.3.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/yargs": "^15.0.5",
    "mocha": "^8.0.1"
  }
}
