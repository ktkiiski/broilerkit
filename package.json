{
  "name": "broilerkit",
  "version": "0.20.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "index.js",
  "types": "index.ts",
  "bin": "./cmd.js",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -f res/cloudformation-migration-lambda.zip && find . -type d \\( -path ./node_modules -o -path ./types \\) -prune -o -type f \\( -name '*.js' -o -name '*.js.map' \\) -exec rm {} \\;",
    "clean-build": "npm run clean && npm run lint && npm run compile",
    "compile": "tsc -p .",
    "zip-cf-migration-lambda": "zip res/cloudformation-migration-lambda.zip -r migration.js node_modules/pg node_modules/buffer-writer node_modules/packet-reader node_modules/pg-connection-string node_modules/pg-pool node_modules/pg-types node_modules/pg-int8 node_modules/postgres-array node_modules/postgres-bytea node_modules/postgres-date node_modules/postgres-interval node_modules/xtend node_modules/pgpass node_modules/split node_modules/through node_modules/semver",
    "prepare": "npm run clean-build && npm run mocha && npm run zip-cf-migration-lambda",
    "lint": "tslint --project .",
    "mocha": "mocha -r ts-node/register spec/**/*.spec.ts",
    "test": "npm run clean && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktkiiski/broilerkit.git"
  },
  "keywords": [
    "broilerplate",
    "boilerplate",
    "aws"
  ],
  "author": {
    "name": "Kimmo Kiiski",
    "email": "kimmo@kii.ski",
    "url": "https://kii.ski"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktkiiski/broilerkit/issues"
  },
  "homepage": "https://github.com/ktkiiski/broilerkit#readme",
  "sideEffects": [
    "./cmd.ts",
    "./cmd.js"
  ],
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jszip": "3.1.6",
    "@types/mime": "^2.0.1",
    "@types/msgpack-lite": "^0.1.7",
    "@types/node": "^10.12.27",
    "@types/node-jose": "^1.1.0",
    "@types/pg": "^7.11.1",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/vinyl-fs": "^2.4.11",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "3.1.6",
    "@types/yamljs": "^0.2.30",
    "aws-sdk": "^2.477.0",
    "base64url": "^3.0.1",
    "chalk": "^2.4.2",
    "core-js-pure": "^3.1.4",
    "css-loader": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.2.1",
    "mime": "^2.4.4",
    "mini-css-extract-plugin": "^0.7.0",
    "msgpack-lite": "^0.1.26",
    "node-jose": "^1.1.3",
    "node-sass": "^4.12.0",
    "pg": "^7.12.1",
    "pg-cursor": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "~3.7.2",
    "ulid": "^2.3.0",
    "url-loader": "^2.0.0",
    "vinyl-fs": "^3.0.3",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^3.7.2",
    "yamljs": "^0.3.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/yargs": "^13.0.0",
    "mocha": "^6.1.4"
  }
}
