{
  "name": "broilerkit",
  "version": "0.14.0",
  "description": "Utility kit for Broilerplate, including helpers for quickly deploying scalable web apps to AWS cloud",
  "main": "index.js",
  "types": "index.ts",
  "bin": "./cmd.js",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "find . -type d \\( -path ./node_modules -o -path ./types \\) -prune -o -type f \\( -name '*.js' -o -name '*.js.map' \\) -exec rm {} \\;",
    "clean-build": "npm run clean && npm run compile && npm run lint",
    "compile": "tsc -p .",
    "prepare": "npm run clean-build && npm run test",
    "lint": "tslint --project .",
    "test": "mocha -r ts-node/register spec/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktkiiski/broilerkit.git"
  },
  "keywords": [
    "broilerplate",
    "boilerplate",
    "aws"
  ],
  "author": {
    "name": "Kimmo Kiiski",
    "email": "kimmo@kii.ski",
    "url": "https://kii.ski"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktkiiski/broilerkit/issues"
  },
  "homepage": "https://github.com/ktkiiski/broilerkit#readme",
  "sideEffects": [
    "./cmd.ts",
    "./cmd.js"
  ],
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jszip": "3.1.6",
    "@types/mime": "^2.0.1",
    "@types/nedb": "^1.8.7",
    "@types/node": "^10.12.27",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/vinyl-fs": "^2.4.11",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "3.1.6",
    "@types/yamljs": "^0.2.30",
    "aws-sdk": "^2.463.0",
    "chalk": "^2.4.2",
    "core-js-pure": "^3.1.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "fast-sass-loader": "^1.4.7",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.2.1",
    "mime": "^2.4.3",
    "mini-css-extract-plugin": "^0.6.0",
    "nedb": "^1.8.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.1.1",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "~3.5.1",
    "ulid": "^2.3.0",
    "url-loader": "^1.1.2",
    "vinyl-fs": "^3.0.3",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "yamljs": "^0.3.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/yargs": "^13.0.0",
    "mocha": "^6.1.4"
  }
}
