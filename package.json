    "jszip": "^3.1.5",
    "markdown-loader": "^2.0.2",
    "mime": "^2.2.0",
    "nedb": "^1.8.0",
    "node-sass": "^4.7.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.3.0",
